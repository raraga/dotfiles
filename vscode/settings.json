{
    "window.zoomLevel": 1,
    "explorer.openEditors.visible": 0,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": false,
    "workbench.activityBar.visible": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.enablePreview": false,
    "editor.fontFamily": "Source Code Pro, SF Mono, Fira Code, Operator Mono,  IBM Plex Mono, Roboto Mono,  Monaco, 'Courier New', monospace, 'Apple Color Emoji'",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 14,
    "workbench.startupEditor": "newUntitledFile",
    "search.location": "panel",





    
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ffc822",
        "statusBar.background": "#12151a",
        "statusBar.noFolderBackground": "#12151a",
        "statusBar.debuggingBackground": "#12151a"
    },
    "emmet.triggerExpansionOnTab": true,
    "blade.format.enable": true,
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.lineHeight": 28,
    "editor.insertSpaces": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "left",
    "git.ignoreLegacyWarning": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderWhitespace": "none",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.minimap.enabled": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.tabWidth": 4,
    "vetur.format.options.tabSize": 4,
    "vetur.format.options.useTabs": true,
    "vim.insertModeKeyBindings" : [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["g", "n"],
            "commands": [
                ":tabn"
            ]
        },
        {
            "before": ["g", "p"],
            "commands": [
                ":tabp"
            ]
        },
        {
            "before": ["g", "d"],
            "commands": [
                "workbench.action.closeWindow"
            ]
        },
    ],
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "editor.lineNumbers": "relative"
}